<section id="gallery" class="py-6 lg:py-12">
    <div class="relative flex flex-col">
            <span class="block text-sm font-semibold tracking-widest text-center md:text-left text-accent mb-xs">GALLERY</span>
            <h2 class="text-4xl font-semibold leading-none tracking-tight text-center text-gray-800 mb-lg md:text-left">Our Photo Gallery</h2>
        <!--Gallery-->
        <div class="grid w-full grid-cols-1 gap-4 md:grid-cols-2 lg:grid-cols-4">
            <!-- Picture 1 -->
            <picture class="sticky top-4 md:relative block overflow-hidden rounded-lg min-h-[16.25rem] max-h-[20rem]">
                <source media="(max-width: 600px)" srcset="./images/massage-service.jpg">
                <source media="(min-width: 601px)" srcset="./images/massage-service.jpg">
                <img width="400" height="320" loading="lazy" decoding="async" src="./images/massage-service.jpg" alt="gallery" class="absolute top-0 left-0 object-cover w-full h-full">
            </picture>
